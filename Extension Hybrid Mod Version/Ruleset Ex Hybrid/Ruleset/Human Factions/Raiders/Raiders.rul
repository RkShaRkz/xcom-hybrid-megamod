      STR_RAIDER_SCOUT_SALVAGE: "s: Rdr. Scout (0/1/1)"
      STR_RAIDER_THUG_SALVAGE: "S: Rdr. Thug (0/1/0)"
      STR_RAIDER_HUMONGOUS_SALVAGE: "S@: Rdr. Humongous (0/2/0)"
